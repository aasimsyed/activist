{
  "name": "activist",
  "private": true,
  "scripts": {
    "build": "nuxi build",
    "build:local": "env-cmd -f ../.env.dev nuxi build",
    "dev": "nuxi dev",
    "dev:local": "env-cmd -f ../.env.dev nuxi dev",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "preview:local": "env-cmd -f ../.env.dev nuxi preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxi prepare",
    "test": "vitest run --run",
    "test:e2e": "cross-env TEST_ENV=local playwright test",
    "test:e2e:prod": "cross-env TEST_ENV=prod playwright test",
    "test:e2e:ui": "cross-env TEST_ENV=local playwright test --ui",
    "test:batches": "./run-test-batches.sh",
    "test:batches:desktop": "./run-test-batches.sh desktop",
    "test:batches:mobile": "./run-test-batches.sh mobile",
    "test:batches:noclean": "SKIP_CLEANUP=true ./run-test-batches.sh",
    "test:batches:desktop:noclean": "SKIP_CLEANUP=true ./run-test-batches.sh desktop",
    "test:batches:mobile:noclean": "SKIP_CLEANUP=true ./run-test-batches.sh mobile",
    "test:batch:unauth": "cross-env TEST_ENV=local playwright test --project unauthenticated-desktop --project unauthenticated-mobile",
    "test:batch:unauth:desktop": "cross-env TEST_ENV=local playwright test --project unauthenticated-desktop",
    "test:batch:unauth:mobile": "cross-env TEST_ENV=local playwright test --project unauthenticated-mobile",
    "test:batch:a11y:auth": "cross-env TEST_ENV=local playwright test --grep @a11y --project chromium --project 'Mobile Chrome'",
    "test:batch:a11y:auth:desktop": "cross-env TEST_ENV=local playwright test --grep @a11y --project chromium",
    "test:batch:a11y:auth:mobile": "cross-env TEST_ENV=local playwright test --grep @a11y --project 'Mobile Chrome'",
    "test:batch:orgs": "cross-env TEST_ENV=local playwright test --grep @org --grep-invert '@slow|@crud|@a11y' --project chromium --project 'Mobile Chrome'",
    "test:batch:orgs:desktop": "cross-env TEST_ENV=local playwright test --grep @org --grep-invert '@slow|@crud|@a11y' --project chromium",
    "test:batch:orgs:mobile": "cross-env TEST_ENV=local playwright test --grep @org --grep-invert '@slow|@crud|@a11y' --project 'Mobile Chrome'",
    "test:batch:crud": "cross-env TEST_ENV=local playwright test --grep @crud --grep-invert @slow --project chromium --project 'Mobile Chrome'",
    "test:batch:crud:desktop": "cross-env TEST_ENV=local playwright test --grep @crud --grep-invert @slow --project chromium",
    "test:batch:crud:mobile": "cross-env TEST_ENV=local playwright test --grep @crud --grep-invert @slow --project 'Mobile Chrome'",
    "test:batch:groups": "cross-env TEST_ENV=local playwright test --grep '@group|@nested' --project chromium --project 'Mobile Chrome'",
    "test:batch:groups:desktop": "cross-env TEST_ENV=local playwright test --grep '@group|@nested' --project chromium",
    "test:batch:groups:mobile": "cross-env TEST_ENV=local playwright test --grep '@group|@nested' --project 'Mobile Chrome'",
    "test:batch:dragdrop": "cross-env TEST_ENV=local playwright test --grep @drag-drop --project chromium --project 'Mobile Chrome'",
    "test:batch:dragdrop:desktop": "cross-env TEST_ENV=local playwright test --grep @drag-drop --project chromium",
    "test:batch:dragdrop:mobile": "cross-env TEST_ENV=local playwright test --grep @drag-drop --project 'Mobile Chrome'",
    "test:desktop": "cross-env TEST_ENV=local playwright test --project chromium --project unauthenticated-desktop",
    "test:mobile": "cross-env TEST_ENV=local playwright test --project 'Mobile Chrome' --project unauthenticated-mobile",
    "test:fast": "cross-env TEST_ENV=local playwright test --grep-invert '@slow|@drag-drop'",
    "test:slow": "cross-env TEST_ENV=local playwright test --grep '@slow|@drag-drop'",
    "typecheck": "nuxi typecheck",
    "format": "yarn prettier . --check --config ../.prettierrc --ignore-path ../.prettierignore",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@headlessui/vue": "1.7.23",
    "@nuxt/devtools": "1.7.0",
    "@nuxt/eslint": "1.2.0",
    "@nuxt/eslint-config": "1.2.0",
    "@nuxt/test-utils": "3.15.4",
    "@nuxtjs/color-mode": "3.5.2",
    "@nuxtjs/device": "3.2.4",
    "@nuxtjs/i18n": "10.0.0",
    "@nuxtjs/tailwindcss": "6.12.2",
    "@pinia/nuxt": "0.9.0",
    "@playwright/test": "1.55.1",
    "@sidebase/nuxt-auth": "1.0.0",
    "@testing-library/vue": "8.1.0",
    "@types/node": "22.12.0",
    "@types/uuid": "10.0.0",
    "@types/zxcvbn": "4.4.5",
    "@vue/test-utils": "2.4.6",
    "@vueuse/core": "12.5.0",
    "@vueuse/nuxt": "12.5.0",
    "axe-core": "4.10.2",
    "axe-html-reporter": "2.2.11",
    "cross-env": "7.0.3",
    "env-cmd": "10.1.0",
    "eslint": "9.22.0",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-vue": "10.0.0",
    "eslint-plugin-vuejs-accessibility": "2.4.1",
    "happy-dom": "15.11.6",
    "lint-staged": "16.1.0",
    "next-auth": "~4.21.1",
    "nuxt": "3.16.1",
    "nuxt-icon": "0.6.10",
    "nuxt-security": "2.2.0",
    "playwright-core": "^1.55.1",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "rollup": "4.32.1",
    "typescript": "5.7.3",
    "vite": "6.1.6",
    "vitest": "3.0.5",
    "vue": "3.5.13",
    "vue-tsc": "3.0.4"
  },
  "dependencies": {
    "@axe-core/playwright": "4.10.0",
    "@maplibre/maplibre-gl-directions": "^0.8.0",
    "@nuxtjs/plausible": "1.2.0",
    "@opentelemetry/api": "1.9.0",
    "@popperjs/core": "2.11.8",
    "@somushq/vue3-friendly-captcha": "1.0.2",
    "@tiptap/core": "2.11.5",
    "@tiptap/extension-link": "2.11.5",
    "@tiptap/extension-mention": "2.11.5",
    "@tiptap/extension-placeholder": "2.11.5",
    "@tiptap/pm": "2.11.5",
    "@tiptap/starter-kit": "2.11.5",
    "@tiptap/suggestion": "2.11.5",
    "@tiptap/vue-3": "2.11.5",
    "@types/geojson": "7946.0.16",
    "@unocss/reset": "65.4.3",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/math": "12.7.0",
    "axios": "1.8.2",
    "commander": "^14.0.1",
    "dompurify": "^3.2.4",
    "dotenv": "16.4.7",
    "eslint-config-prettier": "10.1.1",
    "eslint-flat-config-utils": "2.0.1",
    "floating-vue": "5.2.2",
    "maplibre-gl": "^5.4.0",
    "nuxt-mail": "5.1.1",
    "pinia": "2.3.1",
    "pinia-plugin-persistedstate": "4.2.0",
    "postcss": "8.5.1",
    "postcss-custom-properties": "14.0.4",
    "qrcode": "1.5.4",
    "qrcode.vue": "3.6.0",
    "reduced-motion": "1.0.4",
    "swiper": "11.2.1",
    "tailwind-scrollbar": "3.1.0",
    "tailwindcss": "3.4.17",
    "tippy.js": "6.3.7",
    "tiptap-markdown": "^0.8.10",
    "unocss": "65.4.3",
    "uuid": "11.0.5",
    "v-calendar": "3.1.2",
    "vee-validate": "^4.15.1",
    "vue-i18n": "11.1.10",
    "vue-socials": "2.0.5",
    "vue-sonner": "1.3.0",
    "vuedraggable": "4.1.0",
    "wait-on": "8.0.2",
    "zod": "3.24.1",
    "zxcvbn": "4.4.2"
  },
  "resolutions": {
    "chokidar": "3.6.0",
    "playwright-core": "1.55.1",
    "commander": "14.0.1"
  },
  "packageManager": "yarn@4.7.0"
}
